package pkg

func example1(ghost s set[int]) (ghost b bool) {
  b = s.Contains(42)
}

func example2() {
  assert (set[int] { 1, 2, 3 }).Contains(2)
  assert !(set[int] { 1, 3 }).Contains(2)
  assert (set[bool] { true }).Contains((set[int] { 2 }).Contains(2))
}

func example3(ghost x int, ghost s set[int], ghost t set[int]) {
  assert s.Contains(x) ==> (s union t).Contains(x)
  assert s.Contains(x) ==> t.Contains(x) ==> (s intersection t).Contains(x)
  assert s.Contains(x) ==> s subset t ==> t.Contains(x)
  assert (s intersection t).Contains(x) ==> s.Contains(x) && t.Contains(x)
  assert (s union t).Contains(x) ==> s.Contains(x) || t.Contains(x)
}
